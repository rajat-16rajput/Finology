{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",
  "description": "This is my browser extension.",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
